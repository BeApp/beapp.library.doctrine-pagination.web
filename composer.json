{
  "name": "beapp/doctrine-pagination",
  "description": "Simple Doctrine pagination library for multiple usages",
  "type": "library",
  "license": "MIT",
  "version": "1.0.7",
  "keywords": [
    "doctrine",
    "pagination",
    "select2",
    "datatables",
    "rest"
  ],
  "authors": [
    {
      "name": "Damien Villeneuve",
      "email": "d.villeneuve@beapp.fr"
    }
  ],
  "require": {
      "php": ">=7.2",
      "doctrine/orm": "^2.6",
      "symfony/http-foundation": "^3.0||^4.0||^5.0"
  },
  "autoload": {
    "psr-4": {
      "Beapp\\Doctrine\\Pagination\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Beapp\\Doctrine\\Pagination\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "friendsofsymfony/rest-bundle": "^2.5",
    "phpunit/phpunit": "8.2.4"
  },
  "suggest": {
    "friendsofsymfony/rest-bundle": "^2.5"
  }
}
